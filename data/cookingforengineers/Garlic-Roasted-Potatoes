document.getElementById('categorytd_SousVide').style.backgroundColor='transparent';
document.getElementById('categorytd_TestRecipes').style.border='1px solid #99FF99';
document.getElementById('categorytd_TestRecipes').style.backgroundColor='transparent';

		if (curdisplay != 'block')
		{
			document.getElementById('categorytd_'+id).style.border='1px solid #9999FF';
			document.getElementById('categorytd_'+id).style.backgroundColor='#000060';
			document.getElementById('navcat_'+id).style.display='block';
		}
		else
