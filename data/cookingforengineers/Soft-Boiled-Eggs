	function lb(id)
	{
		document.getElementById(id).style.border='1px solid #FFFF00';
	}
	function db(id)
	{
		document.getElementById(id).style.border='1px solid #99FF99';
	}
	function showcat(id)
	{
		var curdisplay = document.getElementById('navcat_'+id).style.display;
		document.getElementById('navcat_Books').style.display='none';
document.getElementById('navcat_CookingTests').style.display='none';
document.getElementById('navcat_DiningOut').style.display='none';
document.getElementById('navcat_EquipmentGear').style.display='none';
document.getElementById('navcat_KitchenNotes').style.display='none';
document.getElementById('navcat_OffTopic').style.display='none';
document.getElementById('navcat_RecipeFile').style.display='none';
document.getElementById('navcat_SousVide').style.display='none';
document.getElementById('navcat_TestRecipes').style.display='none';

		document.getElementById('categorytd_Books').style.border='1px solid #99FF99';
document.getElementById('categorytd_Books').style.backgroundColor='transparent';
document.getElementById('categorytd_CookingTests').style.border='1px solid #99FF99';
document.getElementById('categorytd_CookingTests').style.backgroundColor='transparent';
document.getElementById('categorytd_DiningOut').style.border='1px solid #99FF99';
document.getElementById('categorytd_DiningOut').style.backgroundColor='transparent';
document.getElementById('categorytd_EquipmentGear').style.border='1px solid #99FF99';
document.getElementById('categorytd_EquipmentGear').style.backgroundColor='transparent';
document.getElementById('categorytd_KitchenNotes').style.border='1px solid #99FF99';
document.getElementById('categorytd_KitchenNotes').style.backgroundColor='transparent';
document.getElementById('categorytd_OffTopic').style.border='1px solid #99FF99';
document.getElementById('categorytd_OffTopic').style.backgroundColor='transparent';
document.getElementById('categorytd_RecipeFile').style.border='1px solid #99FF99';
document.getElementById('categorytd_RecipeFile').style.backgroundColor='transparent';
