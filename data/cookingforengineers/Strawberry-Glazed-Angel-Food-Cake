<div itemscope itemtype="http://data-vocabulary.org/Recipe">
	<div id="articleheader">
			<div class="headertitle"><div>
				<span class="category">
					Recipe File
				</span><br />
				<h1 class="title" style="display:inline" itemprop="name">
					Strawberry Glazed Angel Food Cake
				</h1>
				<span class="author">
					by <span itemprop="author">Michael Chu</span>
				</span>
			</div></div>
		<div class="formatbutton"><a href="/recipe/83/Strawberry-Glazed-Angel-Food-Cake/trn">Recipe Card</a></div>
		<div class="formatbutton"><a href="/recipe/83/Strawberry-Glazed-Angel-Food-Cake/print">Printer-friendly</a></div>
		
		<div class="selectedformatbutton">Normal view</div>
		
		<div class="hr"></div>
	</div>

	<div id="articlebody">
		
		
			<div style="font-size:12pt;font-weight:bold;float:right;margin:0px -6px 0 0;padding:0 4px 6px 8px;background-color:#FFFFA0"><a href="/article/84/NSF-Certification">Next &#187;</a></div>
			<div style="font-size:12pt;font-weight:bold;float:left;margin:0px 0 0 -6px;padding:0 8px 6px 4px;background-color:#FFFFA0"><a href="/article/82/Kitchen-Scales">&#171; Prev</a></div>
			
			<div style="clear:both"></div>
			<div style="float:right;clear:right;padding:4px 0 12px 8px;" class="nothing">
				
<!-- Google DFP Article_160x600-->
<center>
<div id='dfp_gpt_1' style='width:160px; height:600px;'>
